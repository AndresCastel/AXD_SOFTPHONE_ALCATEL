using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace Axede.BussinesObject.Application.Dto
{
    public class DtoLineaBase
    {

        private int _id_linea_base;
        private string _nombre;
        private int _duracion;
        private string _unidad_duracion;
        private DateTime _fecha_inicio;
        private DateTime _fecha_fin;
        private string _ip_switch;
        private int _puerto_switch;

      
        public int Id_linea_base
        {
            get { return _id_linea_base; }
            set { _id_linea_base = value; }
        }
        public string Nombre
        {
            get { return _nombre.Replace("Linea-Base", "LB"); }
            set { _nombre = value; }
        }
        public int Duracion
        {
            get { return _duracion; }
            set { _duracion = value; }
        }
        public string Unidad_duracion
        {
            get { return _unidad_duracion; }
            set { _unidad_duracion = value; }
        }
        public DateTime Fecha_inicio
        {
            get { return _fecha_inicio; }
            set { _fecha_inicio = value; }
        }
        public DateTime Fecha_fin
        {
            get { return _fecha_fin; }
            set { _fecha_fin = value; }
        }
        public string Ip_switch
        {
            get { return _ip_switch; }
            set { _ip_switch = value; }
        }
        public int Puerto_switch
        {
            get { return _puerto_switch; }
            set { _puerto_switch = value; }
        }


    }
}
