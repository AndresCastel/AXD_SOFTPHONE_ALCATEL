<UserControl x:Class="Axede.WPF.Softphone.Applications.GUI.User_Controls.UC_AddContacto"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
              Height="auto"
             Width="auto" Loaded="UserControl_Loaded">
    <StackPanel Orientation="Vertical">
        <Grid VerticalAlignment="Stretch" Height="Auto" HorizontalAlignment="Stretch" Width="Auto">
            <Grid.RowDefinitions>
                <RowDefinition Height="45"/>
                <RowDefinition Height="45"/>
                <RowDefinition Height="45"/>
                <RowDefinition Height="45"/>
                <RowDefinition Height="45*"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="90"/>
                <ColumnDefinition Width="190"/>
                <ColumnDefinition Width="30*"/>
            </Grid.ColumnDefinitions>
            <Label Name="lblNombre" Content="Nombres :" Grid.Row="0" HorizontalAlignment="Right" VerticalAlignment="Center" />
            <TextBox Name="txtNombre"  Grid.Column="1" Grid.Row="0" TabIndex="1" Height="20" MaxLength="50" Margin="0,6,0,0" 
                     ToolTip="{Binding RelativeSource={RelativeSource Self}, Path=Text}" 
                     ToolTipService.ShowOnDisabled="True" PreviewTextInput="txtNombre_PreviewTextInput" 
                      CommandManager.PreviewExecuted="txtNombre_PreviewExecuted"/>
            <Label Name="lblApellido" Content="Apellidos :" Grid.Row="1" HorizontalAlignment="Right" VerticalAlignment="Center" />
            <TextBox Name="txtApellidos"  Grid.Column="1" Grid.Row="1" TabIndex="1" Height="20" MaxLength="50" Margin="0,6,0,0" 
                     ToolTip="{Binding RelativeSource={RelativeSource Self}, Path=Text}" 
                     ToolTipService.ShowOnDisabled="True" PreviewTextInput="txtApellidos_PreviewTextInput"
                     CommandManager.PreviewExecuted="txtApellidos_PreviewExecuted"/>
            <Label Name="lblTelefono" Content="No.Teléfono :" Grid.Row="2" HorizontalAlignment="Right" VerticalAlignment="Center" />
            <TextBox Name="txtTelefono"  Grid.Column="1" Grid.Row="2" TabIndex="1" Height="20" MaxLength="50" Margin="0,6,0,0" 
                     ToolTip="{Binding RelativeSource={RelativeSource Self}, Path=Text}" 
                     ToolTipService.ShowOnDisabled="True" PreviewTextInput="txtTelefono_PreviewTextInput"
                     CommandManager.PreviewExecuted="txtTelefono_PreviewExecuted"/>
            <Label Name="lblExtencion" Content="No.Extensión :" Grid.Row="3" HorizontalAlignment="Right" VerticalAlignment="Center" />
            <TextBox Name="txtExtencion"  Grid.Column="1" Grid.Row="3" TabIndex="1" Height="20" MaxLength="10" Margin="0,6,0,0" 
                     ToolTip="{Binding RelativeSource={RelativeSource Self}, Path=Text}" 
                     ToolTipService.ShowOnDisabled="True" PreviewTextInput="txtExtencion_PreviewTextInput"
                     CommandManager.PreviewExecuted="txtExtencion_PreviewExecuted"/>
            <Label Name="lblTipoTelefono" Content="Tipo Teléfono :" Grid.Row="4" HorizontalAlignment="Right" VerticalAlignment="Center" />
            <ComboBox Name="cmbTipoTelf" Grid.Column="1" Grid.Row="4" TabIndex="1" Height="20" 
                      Width="auto" Margin="0,6,0,0" 
                      ToolTip="{Binding RelativeSource={RelativeSource Self}, Path=Text}" 
                      ToolTipService.ShowOnDisabled="True"/>
            <Label Name="lblNomObliga" Grid.Column="2" HorizontalAlignment="Right" VerticalAlignment="Center" 
                   Style ="{StaticResource LabelStyleObliga}" Margin="0,6,0,0"/>
            <Label Name="lblApeObliga" Grid.Column="2" Grid.Row="1" HorizontalAlignment="Right" VerticalAlignment="Center" 
                   Style ="{StaticResource LabelStyleObliga}" Margin="0,6,0,0"/>
            <Label Name="lblTelfObliga" Grid.Column="2" Grid.Row="2" HorizontalAlignment="Right" VerticalAlignment="Center" 
                   Style ="{StaticResource LabelStyleObliga}" Margin="0,6,0,0"/>
            <Label Name="lblTipoObliga" Grid.Column="2" Grid.Row="4" HorizontalAlignment="Right" VerticalAlignment="Center" 
                   Style ="{StaticResource LabelStyleObliga}" Margin="0,6,0,0"/>
        </Grid>
        <Canvas Height="27" Name="canvas1" Width="176" Margin="18">
            <Button Name="btnGuardar" Height="23" Width="75" Content="" Canvas.Left="8" 
                    Canvas.Top="0" IsManipulationEnabled="True" TabIndex="6" Click="btnGuardar_Click">
            </Button>
            <Button Name="btnLimpiar" Height="23" Width="75" Content="" 
                    Canvas.Left="93" Canvas.Top="0" TabIndex="7" Click="btnLimpiar_Click">
            </Button>
        </Canvas>
    </StackPanel>
</UserControl>
