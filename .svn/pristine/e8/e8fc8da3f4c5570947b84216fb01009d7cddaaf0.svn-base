<Popup xmlns:User_Controls="clr-namespace:Axede.WPF.Softphone.Applications.GUI.User_Controls"  xmlns:Control="clr-namespace:Axede.WPF.Softphone.Applications.UtilControls.PopUp" 
    x:Class="Axede.WPF.Softphone.Applications.UtilControls.PopUp.PopUpControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    x:Name="keyboard"
    DataContext="{Binding ElementName=keyboard}"
    Placement="Right"
    StaysOpen="True"
    AllowsTransparency="True"
    HorizontalOffset="0"
    VerticalOffset="0"
       mc:Ignorable="d"  Width="auto" Height="auto" 
    >
    


    <StackPanel Margin="0"  Width="190" Height="220" >
        <Border CornerRadius="20" Padding="0,9,0,4" BorderThickness="5" RenderTransformOrigin="0.531,0.549" Margin="0,0,93,61" >
            <Border.BorderBrush>
                <LinearGradientBrush EndPoint="0,0" StartPoint="0.5,1.3" >
                    <LinearGradientBrush.GradientStops>
                        <GradientStop Color="#FFC8C8C8" Offset="0"/>
                        <GradientStop Color="#FF747474" Offset="0.495"/>
                        <GradientStop Color="#FFD0D0D0" Offset="0.81"/>
                    </LinearGradientBrush.GradientStops>
                </LinearGradientBrush>
            </Border.BorderBrush>
            <Border.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#FFC8C8C8" Offset="0"/>
                    <GradientStop Color="#FF292929" Offset="0.314"/>
                    <GradientStop Color="#FFD0D0D0" Offset="0.81"/>
                </LinearGradientBrush>
            </Border.Background>
        </Border>

  <User_Controls:Video_UC />

    </StackPanel>

</Popup>

