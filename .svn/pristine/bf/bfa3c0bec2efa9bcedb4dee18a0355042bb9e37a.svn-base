<UserControl x:Class="Axede.WPF.Softphone.Applications.GUI.User_Controls.Botonera__UC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             Height="auto" Width="auto">
    <Canvas Height="178" Width="185">

        <Grid Width="180" Height="168">
            <Grid.RowDefinitions >
                <RowDefinition  Height="40" />
                <RowDefinition Height="40"/>
                <RowDefinition Height="40"/>
                <RowDefinition  Height="40"/>

            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="53"/>
                <ColumnDefinition Width="53" />
                <ColumnDefinition Width="53"/>
                <ColumnDefinition Width="53"/>
            </Grid.ColumnDefinitions>
            <Button Name="Btn1" Style="{StaticResource GlassButtonSem}" Grid.Row="0" Grid.Column="0"  Height="32" Width="44" HorizontalAlignment="Center" Background="White" Click="BtnAccion_Click"  >
                <StackPanel Orientation="Horizontal" Margin="0,0,0,0">
                    <Label Content="1" Foreground="#FF1A341B" FontFamily="Stencil" FontSize="20" Margin="-10,0,0,0"/>
                    <Label VerticalAlignment="Center" Foreground="#FFBD8010" FontFamily="Stencil" FontSize="9"/>
                </StackPanel>
                <Button.Effect>
                    <DropShadowEffect BlurRadius="10" ShadowDepth="5"/>
                </Button.Effect>
            </Button>
            <Button Name="Btn2" Style="{StaticResource GlassButtonSem}" Height="32" Width="44" Grid.Row="0" HorizontalAlignment="Center" Grid.Column="1"  Background="White"   Click="BtnAccion_Click" >
                <StackPanel Orientation="Horizontal" >
                    <Label Content="2" Foreground="#FF1A341B" FontFamily="Stencil" FontSize="20"/>
                    <Label VerticalAlignment="Center" Margin="-4,0,0,0" Content="abc" FontSize="9" Foreground="#FFBD8010" FontFamily="Stencil"/>
                </StackPanel>
                <Button.Effect>
                    <DropShadowEffect BlurRadius="10" ShadowDepth="5"/>
                </Button.Effect>
            </Button>
            <Button Name="Btn4" Style="{StaticResource GlassButtonSem}" Grid.Row="1" HorizontalAlignment="Center" Grid.Column="0"  Height="32" Width="44"  Background="White"   Click="BtnAccion_Click" Margin="3,2,0,6" >
                <StackPanel Orientation="Horizontal" >
                    <Label Content="4" Foreground="#FF1A341B" FontFamily="Stencil" FontSize="20"/>
                    <Label VerticalAlignment="Center" Content="ghi" Margin="-4,0,0,0" FontSize="9" Foreground="#FFBD8010" FontFamily="Stencil"/>
                </StackPanel>
                <Button.Effect>
                    <DropShadowEffect BlurRadius="10" ShadowDepth="5"/>
                </Button.Effect>
            </Button>
            <Button Name="Btn3" Style="{StaticResource GlassButtonSem}" Grid.Row="0" HorizontalAlignment="Center" Grid.Column="2" Height="32" Width="44"  Background="White"   Click="BtnAccion_Click">
                <StackPanel Orientation="Horizontal" >
                    <Label Content="3" Foreground="#FF1A341B" FontFamily="Stencil" FontSize="20"/>
                    <Label VerticalAlignment="Center" Margin="-4,0,0,0" Content="def" Foreground="#FFBD8010" FontSize="9" FontFamily="Stencil"/>
                </StackPanel>
                <Button.Effect>
                    <DropShadowEffect BlurRadius="10" ShadowDepth="5"/>
                </Button.Effect>
            </Button>
            <Button Name="Btn5" Style="{StaticResource GlassButtonSem}" Grid.Row="1" HorizontalAlignment="Center" Grid.Column="1"  Height="32" Width="44"  Background="White"   Click="BtnAccion_Click" Margin="3,3,0,0" VerticalAlignment="Top" >
                <StackPanel Orientation="Horizontal" >
                    <Label Content="5" Foreground="#FF1A341B" FontFamily="Stencil" FontSize="20"/>
                    <Label VerticalAlignment="Center" Margin="-4,0,0,0" Content="jkl" FontSize="9" Foreground="#FFBD8010" FontFamily="Stencil"/>
                </StackPanel>
                <Button.Effect>
                    <DropShadowEffect BlurRadius="10" ShadowDepth="5"/>
                </Button.Effect>
            </Button>

            <Button Name="Btn6" Style="{StaticResource GlassButtonSem}" Grid.Row="1" HorizontalAlignment="Center" Grid.Column="2"  Height="32" Width="44"  Background="White"   Click="BtnAccion_Click" Margin="3,3,0,0" VerticalAlignment="Top">
                <StackPanel Orientation="Horizontal" >
                    <Label Content="6" Foreground="#FF1A341B" FontFamily="Stencil" FontSize="20"/>
                    <Label VerticalAlignment="Center" Margin="-4,0,0,0" Content="mno"  FontSize="9" Foreground="#FFBD8010" FontFamily="Stencil"/>
                </StackPanel>
                <Button.Effect>
                    <DropShadowEffect BlurRadius="10" ShadowDepth="5"/>
                </Button.Effect>
            </Button>

            <Button Name="Btn7" Style="{StaticResource GlassButtonSem}" Grid.Row="2" HorizontalAlignment="Center" Grid.Column="0"  Height="32" Width="44"  Background="White"   Click="BtnAccion_Click" Margin="3,3,0,5">
                <StackPanel Orientation="Horizontal" >
                    <Label Content="7" Foreground="#FF1A341B" FontFamily="Stencil" FontSize="20"/>
                    <Label VerticalAlignment="Center" Margin="-7,0,0,0" Content="pqrs" FontSize="9" Foreground="#FFBD8010" FontFamily="Stencil"/>
                </StackPanel>
                <Button.Effect>
                    <DropShadowEffect BlurRadius="10" ShadowDepth="5"/>
                </Button.Effect>
            </Button>

            <Button Name="Btn8" Style="{StaticResource GlassButtonSem}" Grid.Row="2" HorizontalAlignment="Center" Grid.Column="1"  Height="32" Width="44"  Background="White"   Click="BtnAccion_Click" Margin="3,3,0,5">
                <StackPanel Orientation="Horizontal" >
                    <Label Content="8" Foreground="#FF1A341B" FontFamily="Stencil" FontSize="20"/>
                    <Label VerticalAlignment="Center" Margin="-4,0,0,0" Content="tuv" Foreground="#FFBD8010" FontSize="9" FontFamily="Stencil"/>
                </StackPanel>
                <Button.Effect>
                    <DropShadowEffect BlurRadius="10" ShadowDepth="5"/>
                </Button.Effect>
            </Button>

            <Button Name="Btn9" Style="{StaticResource GlassButtonSem}" Grid.Row="2" HorizontalAlignment="Center" Grid.Column="2"  Height="32" Width="44"  Background="White"   Click="BtnAccion_Click" Margin="3,3,0,5">
                <StackPanel Orientation="Horizontal" >
                    <Label Content="9" Foreground="#FF1A341B" FontFamily="Stencil" FontSize="20"/>
                    <Label VerticalAlignment="Center" Margin="-7,0,0,0" Content="wxyz" FontSize="9" Foreground="#FFBD8010" FontFamily="Stencil"/>
                </StackPanel>
                <Button.Effect>
                    <DropShadowEffect BlurRadius="10" ShadowDepth="5"/>
                </Button.Effect>
            </Button>

            <Button Name="BtnX" Style="{StaticResource GlassButtonSem}" Grid.Row="3" HorizontalAlignment="Center" Grid.Column="0"  Height="32" Width="44"  Background="White"   Click="BtnAccion_Click" Margin="3,3,0,0" VerticalAlignment="Top">
                <StackPanel Orientation="Horizontal" >
                    <Label Content="*" Foreground="#FF1A341B" FontFamily="Stencil" VerticalAlignment="Top" FontSize="30"/>
                </StackPanel>
                <Button.Effect>
                    <DropShadowEffect BlurRadius="10" ShadowDepth="5"/>
                </Button.Effect>
            </Button>

            <Button Name="Btn0" Style="{StaticResource GlassButtonSem}" Grid.Row="3" HorizontalAlignment="Center" Grid.Column="1"  Height="32" Width="44"  Background="White"   Click="BtnAccion_Click" Margin="3,3,0,0" VerticalAlignment="Top">
                <StackPanel Orientation="Horizontal" >
                    <Label Content="0" Foreground="#FF1A341B" FontFamily="Stencil" FontSize="20"/>
                </StackPanel>
                <Button.Effect>
                    <DropShadowEffect BlurRadius="10" ShadowDepth="5"/>
                </Button.Effect>
            </Button>

            <Button Name="BtnNum" Style="{StaticResource GlassButtonSem}" Grid.Row="3" HorizontalAlignment="Center" Grid.Column="2"  Height="32" Width="44"  Background="White"   Click="BtnAccion_Click" Margin="3,3,0,0" VerticalAlignment="Top">
                <StackPanel Orientation="Horizontal" >
                    <Label Content="#" Foreground="#FF1A341B" FontFamily="Stencil" FontSize="20"/>
                </StackPanel>
                <Button.Effect>
                    <DropShadowEffect BlurRadius="10" ShadowDepth="5"/>
                </Button.Effect>
            </Button>
        </Grid>
    </Canvas>
</UserControl>
