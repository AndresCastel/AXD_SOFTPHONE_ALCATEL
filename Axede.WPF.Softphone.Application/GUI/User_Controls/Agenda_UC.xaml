<UserControl x:Class="Axede.WPF.Softphone.Applications.GUI.User_Controls.Agenda_UC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             Height="auto" Width="auto">
   
        <Grid Background="Transparent">
        <Grid.RowDefinitions >
            <RowDefinition  Height="55*"/>
            <RowDefinition  Height="55*"/>
            <RowDefinition  Height="55*"/>
            <RowDefinition  Height="55*"/>
            <RowDefinition  Height="55*"/>
            <RowDefinition  Height="55*"/>
            <RowDefinition  Height="55*"/>
            <RowDefinition  Height="55*"/>
            <RowDefinition  Height="55*"/>
            <RowDefinition  Height="55*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="auto" />
            <ColumnDefinition Width="auto" />
        </Grid.ColumnDefinitions>
        <TextBox Grid.Column="0" Grid.Row="0" HorizontalAlignment="Left" Margin="3"  Width="200" Height="23" Name="txtAgenda1"/>
        <TextBox Grid.Column="0" Grid.Row="1" HorizontalAlignment="Left" Margin="3"  Width="200" Height="23" Name="txtAgenda2"/>
        <TextBox Grid.Column="0" Grid.Row="2" HorizontalAlignment="Left" Margin="3"  Width="200" Height="23" Name="txtAgenda3"/>
        <TextBox Grid.Column="0" Grid.Row="3" HorizontalAlignment="Left" Margin="3"  Width="200" Height="23" Name="txtAgenda4"/>
        <TextBox Grid.Column="0" Grid.Row="4" HorizontalAlignment="Left" Margin="3"  Width="200" Height="23" Name="txtAgenda5"/>
        <TextBox Grid.Column="0" Grid.Row="5" HorizontalAlignment="Left" Margin="3"  Width="200" Height="23" Name="txtAgenda6"/>
        <TextBox Grid.Column="0" Grid.Row="6" HorizontalAlignment="Left" Margin="3"  Width="200" Height="23" Name="txtAgenda7"/>
        <TextBox Grid.Column="0" Grid.Row="7" HorizontalAlignment="Left" Margin="3"  Width="200" Height="23" Name="txtAgenda8"/>
        <TextBox Grid.Column="0" Grid.Row="8" HorizontalAlignment="Left" Margin="3"  Width="200" Height="23" Name="txtAgenda9"/>
        <TextBox Grid.Column="0" Grid.Row="9" HorizontalAlignment="Left" Margin="3"  Width="200" Height="23" Name="txtAgenda10"/>
        <Button Name="BtnAgenda1" Style="{StaticResource GlassButtonSem}" Grid.Row="0" Grid.Column="1" Width="73" Height="25" HorizontalAlignment="Left" Background="White" Click="BtnLlamarAgenda_Click" Margin="0,0,0,8"  >           
            <Button.Effect>
                <DropShadowEffect BlurRadius="10" ShadowDepth="5"/>
            </Button.Effect>
        </Button>
        <Button Name="BtnAgenda2" Style="{StaticResource GlassButtonSem}" Grid.Row="1" Grid.Column="1" Width="73" Height="25" HorizontalAlignment="Left" Background="White" Click="BtnLlamarAgenda_Click" Margin="0,0,0,8"  >
            <Button.Effect>
                <DropShadowEffect BlurRadius="10" ShadowDepth="5"/>
            </Button.Effect>
        </Button>
        <Button Name="BtnAgenda3" Style="{StaticResource GlassButtonSem}" Grid.Row="2" Grid.Column="1" Width="73" Height="25" HorizontalAlignment="Left" Background="White" Click="BtnLlamarAgenda_Click" Margin="0,0,0,8"  >
            <Button.Effect>
