            <Button.Effect>
                <DropShadowEffect BlurRadius="10" ShadowDepth="5"/>
            </Button.Effect>
        </Button>
        <Button Name="BtnAgenda10" Style="{StaticResource GlassButtonSem}" Grid.Row="9" Grid.Column="1" Width="73" Height="25" HorizontalAlignment="Left" Background="White" Click="BtnLlamarAgenda_Click" Margin="0,0,0,8"  >
            <Button.Effect>
                <DropShadowEffect BlurRadius="10" ShadowDepth="5"/>
            </Button.Effect>
        </Button>
        </Grid>
</UserControl>
