        <Button Name="BtnAgenda4" Style="{StaticResource GlassButtonSem}" Grid.Row="3" Grid.Column="1" Width="73" Height="25" HorizontalAlignment="Left" Background="White" Click="BtnLlamarAgenda_Click" Margin="0,0,0,8"  >
            <Button.Effect>
