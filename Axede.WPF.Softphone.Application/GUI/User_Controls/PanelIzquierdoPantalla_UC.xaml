<UserControl x:Class="Axede.WPF.Softphone.Applications.GUI.User_Controls.PanelIzquierdoPantalla_UC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             Height="auto" Width="auto">
    <StackPanel>
        <Button x:Name="BtnPanelDer1" Margin="0,0,0,30" Style="{StaticResource GlassButtonSem}"  Background="White"   Click="BtnAccion_Click" Height="17" Width="60">

            <Button.Effect>
                <DropShadowEffect BlurRadius="10" ShadowDepth="5"/>
            </Button.Effect>
        </Button>
        <Button x:Name="BtnPanelDer2" Margin="0,0,0,30" Style="{StaticResource GlassButtonSem}"  Background="White"   Click="BtnAccion_Click" Height="17" Width="60"  >

            <Button.Effect>
                <DropShadowEffect BlurRadius="10" ShadowDepth="5"/>
            </Button.Effect>
        </Button>
        <Button x:Name="BtnPanelDer3" Margin="0,0,0,0" Style="{StaticResource GlassButtonSem}"  Background="White"   Click="BtnAccion_Click" Height="17" Width="60" >

            <Button.Effect>
                <DropShadowEffect BlurRadius="10" ShadowDepth="5"/>
            </Button.Effect>
        </Button>
    </StackPanel>
</UserControl>
